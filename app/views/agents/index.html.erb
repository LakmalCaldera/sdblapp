<section class="inner-padding">
  <button type="button" class="btn btn-primary app-btn" data-toggle="modal" data-target="#exampleModal" data-whatever="@mdo"><i class="fa fa-plus" aria-hidden="true"></i></button>
  <input type="text" id="app-search" class="light-table-filter" placeholder="Search.." data-table="order-table">
<table id="app-table" class="order-table table">
  <thead>
  <tr class="header">
    <th>#</th>
    <th>Account</th>
    <th>Branch</th>
    <th>Created</th>
    <th>Updated</th>
    <th>Actions</th>
  </tr>
  </thead>
  <tbody>
  <% @agents.each_with_index do |agent, index| %>
      <tr>
        <th scope="row"><%= index %></th>
        <td><%= agent.account %></td>
        <td><%= agent.branch %></td>
        <td><%= agent.created_at %></td>
        <td><%= agent.updated_at %></td>
        <td>
          <%= link_to '<i class="fa fa-pencil teal-text"></i>'.html_safe, edit_agent_path(agent) %>
          <%= link_to '<i class="fa fa-times red-text"></i>'.html_safe, agent, method: :delete, data: {confirm: 'Are you sure?'} %>
        </td>
      </tr>
  <% end %>
  </tbody>
</table>
</section>


<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Create a new Agent</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%= form_for(@agent_item) do |f| %>
            <div class="field">
              <%= f.label :account %>
              <%= f.text_field :account, class: 'form-input' %>
            </div>
            <div class="field">
              <%= f.label :branch %>
              <%= f.text_field :branch, class: 'form-input' %>
            </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <%= f.submit :class => "btn btn-primary"  %>
      </div>
              <% end %>
    </div>
  </div>
</div>

<style type="text/css">
